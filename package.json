{
  "name": "cj-scripts",
  "version": "0.2.0",
  "description": "A React CLI for computer gangsters",
  "repository": "https://github.com/cullenjett/cj-scripts",
  "author": "Cullen Jett <cullenjett@gmail.com>",
  "keywords": [],
  "license": "MIT",
  "bin": {
    "cj-scripts": "src/index.js"
  },
  "scripts": {
